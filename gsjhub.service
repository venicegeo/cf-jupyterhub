[Unit]
Description=Geoint Services Jupyterhub
After=network.service

[Service]
User=gsjhub
Group=gsjhub
ExecStart=/opt/gsjhub/bin/jupyterhub --config /etc/gsjhub/jupyterhub_config.py
Environment=PATH=/opt/gsjhub/bin:$PATH
WorkingDirectory=/var/run/gsjhub
Restart=always

[Install]
WantedBy=multi-user.target
