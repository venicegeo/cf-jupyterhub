[Unit]
Description=Geoint Services Jupyterhub
After=network.service

[Service]
User=gsjhub
Group=gsjhub
ExecStart=/opt/gsjhub/bin/jupyterhub --config /etc/gsjhub/jupyterhub-ldap_config.py
EnvironmentFile=/etc/gsjhub/gsjhub.env
WorkingDirectory=/opt/gsjhub
Restart=always

[Install]
WantedBy=multi-user.target
